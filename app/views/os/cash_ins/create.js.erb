<% if @cash_in.errors.any? %>
  $('#common_modal #common-errors').remove();
  $('#common_modal .modal-body').prepend("<%= j( render partial: 'shared/common_errors', locals: { model: @cash_in } ) %>");
<% else %>
  $('#common_modal').modal('hide');
  $('tbody tr:first').after("<%= j(render @cash_in) %>");
<% end %>
