<% if @labor.errors.any? %>
  $('#common_modal #common-errors').remove();
  $('#common_modal .modal-body').prepend("<%= j( render partial: 'shared/common_errors', locals: { model: @labor } ) %>");
<% else %>
  $('#common_modal').modal('hide');
  $("#labor-<%=@labor.id%>").replaceWith("<%= j(render @labor) %>")
<% end %>